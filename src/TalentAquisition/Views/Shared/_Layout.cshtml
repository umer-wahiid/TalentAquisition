<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - TalentAquisition</title>

    <link rel="stylesheet" href="~/TalentAquisition.styles.css" asp-append-version="true" />

    <link href="~/css/bootstrap.min.css" rel="stylesheet">

    <link rel="stylesheet" href="~/css/poppins-font.css" />

    <link rel="stylesheet" href="~/css/font-awesome-all.css" />

    <link rel="stylesheet" href="~/css/themify-icons.css" />

    <link rel="stylesheet" href="~/css/icofont.css">

    <link href="~/css/vendors/toastr.css" rel="stylesheet" asp-append-version="true" />

    <link rel="stylesheet" type="text/css" href="~/css/vendors/datatables.css" asp-append-version="true" />
    
    <link rel="stylesheet" href="~/css/dx.common.css" asp-append-version="true">
    
    <link rel="stylesheet" href="~/css/dx.generic.custom-scheme.css" asp-append-version="true">
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">TalentAquisition</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2025 - TalentAquisition - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>

    <script src="https://code.jquery.com/jquery-3.7.1.min.js" integrity="sha256-/JqT3SQfawRcv/BIHPThkBvs0OEvtFFmqPF/lYI/Cxo=" crossorigin="anonymous" asp-append-version="true"></script>
    <script src="~/js/jquery-ui.min.js" asp-append-version="true"></script>
    <script src="~/js/toastr.min.js" asp-append-version="true"></script>
    <script src="~/js/toastr.script.min.js" asp-append-version="true"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js" asp-append-version="true"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js" asp-append-version="true"></script>
    <script src="~/js/perfect-scrollbar.min.js" asp-append-version="true"></script>
    <script src="~/js/main.js" asp-append-version="true"></script>
    <script src="~/js/input-mask.js" asp-append-version="true"></script>
    <script src="~/js/bootstrap.bundle.min.js" asp-append-version="true"></script>
    <script src="~/js/datatable/datatables/jquery.dataTables.min.js" asp-append-version="true"></script>
    <script src="~/js/datatable/datatables/datatable.custom.js" asp-append-version="true"></script>
    <script type="text/javascript"
            src="https://cdnjs.cloudflare.com/ajax/libs/jspdf/2.5.1/jspdf.umd.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/babel-polyfill/7.12.1/polyfill.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/exceljs/4.3.0/exceljs.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/FileSaver.js/2.0.5/FileSaver.min.js"></script>
    <script src="~/js/dx.all.js" asp-append-version="true"></script>
    <script src="~/customjs/tas_helper.js"></script>
    <script src="~/customjs/tas_ajaxHelper.js"></script>
    <script>
            window.jsPDF = window.jspdf.jsPDF;
    </script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
