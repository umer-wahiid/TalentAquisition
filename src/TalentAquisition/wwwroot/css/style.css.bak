ul.ul-leftside {
    list-style: none;
    padding: 0;
    margin: 0;
    max-height: calc(100vh - 175px);
    overflow-y: auto;
}

    ul.ul-leftside > li > a {
        display: block;
        padding: 2px 8px 2px 0;
        text-decoration: none !important;
        color: var(--theme-black);
        border-bottom: 1px solid #efefef;
        font-size: 11px;
        font-weight: 400;
        position: relative;
        -webkit-transition: all .3s cubic-bezier(0,0,0,.24) !important;
        transition: all .3s cubic-bezier(0,0,0,.24) !important;
    }

        ul.ul-leftside > li > a:hover {
            color: var(--theme-blue);
            background-color: #dce8ee;
        }

        ul.ul-leftside > li > a:before {
            content: "\e661";
            font-family: themify;
            color: #055a87;
            position: absolute;
            left: 2px;
            opacity: 0;
            -webkit-transition: all .3s cubic-bezier(0,0,0,.24) !important;
            transition: all .3s cubic-bezier(0,0,0,.24) !important;
            font-size: 11px;
        }

    ul.ul-leftside li {
        padding: 0 10px;
    }

    ul.ul-leftside > li > a:hover, ul.ul-leftside > li > a.active {
        padding-left: 15px;
    }

        ul.ul-leftside > li > a:hover:before, ul.ul-leftside > li > a.active:before {
            opacity: 1;
        }

