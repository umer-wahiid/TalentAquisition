:root {
    --theme-blue: #055a87;
    --theme-black: #04003b;
    --theme-red: red;
    --theme-green: green;
    --theme-white: #ffffff;
}


body {
    background-color: rgb(4 0 59 / 7%);
    font-family: 'Poppins', sans-serif;
    overflow: hidden;
}

::-webkit-scrollbar-track {
    -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3);
    border-radius: 10px;
    background-color: #F5F5F5;
}

::-webkit-scrollbar {
    width: 5px;
    background-color: var(--theme-blue);
}

::-webkit-scrollbar-thumb {
    border-radius: 10px;
    -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,.3);
    background-color: var(--theme-blue);
}

.contain_wrap {
    /* padding: 15px 0px; */
    /* height: calc(100vh - 100px); */
    /* overflow-y: auto; */
}

.headertopbar {
    background-color: #fff;
    /* box-shadow: 0px 5px 11px 0px rgb(51 51 51 / 33%); */
}

.brang_logo {
    width: 163px;
    padding: 10px 0px;
}

.nav.ul-navbar {
    background-color: var(--theme-blue);
    position: relative;
    z-index: 100;
    padding: 0px 0;
    /* justify-content: center; */
    box-shadow: 0 6px 20px 0 rgb(0 0 0 / 19%), 0 8px 17px 0 rgb(0 0 0 / 20%);
}

    .nav.ul-navbar li.nav-item a.nav-link {
        font-size: 11px;
        padding: 6px 12px;
        text-decoration: none;
        color: var(--theme-white);
    }

        .nav.ul-navbar li.nav-item a.nav-link > i {
            margin-right: 5px;
        }

.ul-navbar ul.dropdown-menu {
    padding: 0;
    border: 0;
    box-shadow: 1px 1px 8px 1px rgb(51 51 51 / 16%);
    border-radius: 0;
    min-width: 200px;
    max-width: 100%;
}

    .ul-navbar ul.dropdown-menu a {
        color: var(--theme-black);
    }

.ul-navbar .dropdown-menu li {
    position: relative;
    padding: 0 !important;
    width: 100%;
    display: inline-block;
    opacity: 1 !important;
}

.ul-navbar .dropdown-menu .submenu {
    display: none;
    position: absolute;
    left: 100%;
    top: -7px;
}

.ul-navbar .dropdown-menu .submenu-left {
    right: 100%;
    left: auto;
}

.ul-navbar .dropdown-menu > li:hover {
    background-color: #f1f1f1
}

    .ul-navbar .dropdown-menu > li:hover > .submenu {
        display: flex;
        flex-direction: column;
    }

.btn-cstm {
    background-color: var(--theme-blue);
    color: var(--theme-white);
    border-radius: 0;
}

.img-user {
    width: 30px;
    height: 30px;
    border-radius: 50px;
    float: left;
    margin-right: 10px;
    margin-top: 4px;
}


.dropdown .dropdown-menu .dropdown-item {
    opacity: 1;
}

.useranam {
    display: inline-block;
    text-align: left;
    font-size: 14px;
    font-weight: 600;
    line-height: 18px;
}

    .useranam .useradmin {
        display: block;
        font-weight: 400;
    }

.dropdown-toggle::after {
    border: 0;
    content: "\f107";
    font: normal normal normal 14px/1 FontAwesome;
    font-size: 11px;
    font-weight: 700;
    margin-left: 5px;
    position: absolute;
    top: 10px;
}

ul.dropdown-menu li a {
    text-decoration: none;
    display: flex;
    padding: 6px 13px;
    width: 100%;
    color: var(--theme-white);
    white-space: normal;
    align-items: center;
    font-size: 11px;
    -webkit-transition: all .3s cubic-bezier(0,0,0,.24) !important;
    transition: all .3s cubic-bezier(0,0,0,.24) !important;
}

    ul.dropdown-menu li a:hover, ul.dropdown-menu li a.active {
        padding-left: 20px !important;
    }

ul.dropdown-menu li a:before {
    content: "\e661";
    font-family: themify;
    color: #055a87;
    position: absolute;
    left: 5px;
    opacity: 0;
    -webkit-transition: all .3s cubic-bezier(0,0,0,.24) !important;
    transition: all .3s cubic-bezier(0,0,0,.24) !important;
    font-size: 11px;
}

    ul.dropdown-menu li a:hover:before, ul.dropdown-menu li a.active:before {
        opacity: 1;
    }

    .dropdown-menu li {
        display: inline-block;
        width: 100%;
    }

    .dropdown-menu li a .fa {
        width: 30px;
        height: 30px;
        background-color: #ebeef4;
        border-radius: 50px;
        text-align: center;
        line-height: 20px;
        padding: 5px 8px;
        margin-right: 7px;
    }

    .dropdown-menu li a:hover {
        background-color: #dce8ee !important;
        color: var(--theme-blue) !important;
    }

        .dropdown-menu li a:hover .fa {
            background-color: #ebeef4;
            color: #333;
        }

.nav.ul-navbar li a:hover, .nav.ul-navbar li a.active {
    background-color: var(--theme-black);
    color: var(--theme-white);
    padding: 6px 10px;
}

.topexlink {
    list-style: none;
    background-color: rgb(4 0 59 / 7%);
    display: inline-block;
    margin-bottom: 0;
